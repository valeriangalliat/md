# We don't want to merge any parent configuration
extend: false

lang: en
layout: layout.mustache

# Highlight code blocks
codehilite: true

# Always display lines
codehilite_linenums: true

# Markdown headers will behin at level 2
header_level: 2

# Headers links to themselves
# header_anchorlink: true

# Add a permalink after headers (only on hover with current layout)
header_permalink: true

# Title of the table of contents
# toc_title: Custom title
